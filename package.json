{
  "name": "cypress-code-coverage-v8",
  "version": "0.0.0-development",
  "description": "Saves the code coverage collected during Cypress tests",
  "scripts": {
    "start": "parcel serve cypress/index.html",
    "coverage:verify": "npx nyc report --check-coverage true --lines 80",
    "cy:open": "cypress open",
    "watch": "tsc --watch",
    "build": "tsc",
    "dev": "start-test 1234 cy:open",
    "semantic-release": "semantic-release",
    "test": "start-test 1234 'npx cypress run'",
    "report:coverage": "nyc report --reporter=html",
    "dev:no:coverage": "start-test 1234 'cypress open --env coverage=false'",
    "format": "prettier --write '*.js'",
    "format:check": "prettier --check '*.js'",
    "check:markdown": "find *.md -exec npx markdown-link-check {} \\;",
    "effective:config": "circleci config process .circleci/config.yml | sed /^#/d"
  },
  "main": "dist/support.js",
  "types": "dist/support.d.ts",
  "exports": {
    ".": {
      "types": "./dist/support.d.ts",
      "browser": "./dist/support.js",
      "node": "./dist/support.js",
      "require": "./dist/support.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "browser": "./dist/plugin.js",
      "node": "./dist/plugin.js",
      "require": "./dist/plugin.js"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "browser": "./dist/register.js",
      "node": "./dist/register.js",
      "require": "./dist/register.js"
    },
    "./middlewares/express": {
      "types": "./dist/middlewares/express.d.ts",
      "browser": "./dist/middlewares/express.js",
      "node": "./dist/middlewares/express.js",
      "require": "./dist/middlewares/express.js"
    },
    "./middlewares/koa": {
      "types": "./dist/middlewares/koa.d.ts",
      "browser": "./dist/middlewares/koa.js",
      "node": "./dist/middlewares/koa.js",
      "require": "./dist/middlewares/koa.js"
    },
    "./middlewares/hapi": {
      "types": "./dist/middlewares/hapi.d.ts",
      "browser": "./dist/middlewares/hapi.js",
      "node": "./dist/middlewares/hapi.js",
      "require": "./dist/middlewares/hapi.js"
    },
    "./middlewares/nextjs": {
      "types": "./dist/middlewares/nextjs.d.ts",
      "browser": "./dist/middlewares/nextjs.js",
      "node": "./dist/middlewares/nextjs.js",
      "require": "./dist/middlewares/nextjs.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "plugin",
    "register",
    "middlewares"
  ],
  "peerDependencies": {
    "cypress": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rohit-gohri/cypress-code-coverage-v8.git"
  },
  "keywords": [
    "cypress",
    "istanbul",
    "cypress-plugin",
    "code",
    "coverage",
    "v8"
  ],
  "author": "Rohit Gohri <code@rohit.page>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rohit-gohri/cypress-code-coverage-v8/issues"
  },
  "homepage": "https://github.com/rohit-gohri/cypress-code-coverage-v8#readme",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "dependencies": {
    "chalk": "4.1.2",
    "chrome-remote-interface": "^0.33.0",
    "dayjs": "1.11.10",
    "debug": "4.3.4",
    "execa": "4.1.0",
    "globby": "11.1.0",
    "istanbul-lib-coverage": "^3.2.2",
    "js-yaml": "4.1.0",
    "nyc": "15.1.0",
    "slug": "5.1.0",
    "v8-to-istanbul": "^9.2.0"
  },
  "devDependencies": {
    "@hapi/hapi": "^21.3.3",
    "@types/chrome-remote-interface": "^0.31.14",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/koa": "2.13.4",
    "@types/node": "^20.11.19",
    "check-code-coverage": "1.10.5",
    "console-log-div": "0.6.3",
    "cypress": "^13.1.0",
    "devtools-protocol": "^0.0.1273771",
    "express": "^4.18.2",
    "koa": "2.13.4",
    "lodash": "4.17.21",
    "markdown-link-check": "3.12.2",
    "next": "^13.0.0",
    "parcel-bundler": "1.12.5",
    "prettier": "3.2.5",
    "rimraf": "5.0.8",
    "semantic-release": "17.4.7",
    "serve": "14.2.1",
    "start-server-and-test": "2.0.4",
    "typescript": "^5.4.2",
    "webpack": "^5.68.0",
    "webpack-cli": "^5.1.4"
  }
}
